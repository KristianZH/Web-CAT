        },
        {
            property    = useEnhancedFeedback;
            type        = boolean;
            advanced    = true;
            default     = false;
            name        = "Use Enhanced Feedback (Experimental)";
            category    = "Developer Settings";
            description =
  "Set to true to use the new (experimental) enhanced feedback layout.";
        }
    );
